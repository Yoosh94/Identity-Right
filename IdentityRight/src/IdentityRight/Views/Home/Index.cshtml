@{
    ViewData["Title"] = "Home Page";
}
}
<!--
    
-->
<div class="home-body-image"></div>
<div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="6000">
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
        <li data-target="#myCarousel" data-slide-to="3"></li>
    </ol>
    <div class="carousel-inner carousel-style" role="listbox">
        <div class="item active">
            <div class="carousel-item3"></div>
            <div class="carousel-caption">
                <p>
                    Learn how to update your address with an organisation.
                    <a class="btn btn-default btn-default" href="#">
                        Learn More
                    </a>
                </p>
            </div>
        </div>
        <div class="item">
            <div class="carousel-item3"></div>
            <div class="carousel-caption">
                <p>
                    Learn how important it is to update your address when you move.
                    <a class="btn btn-default btn-default" href="#">
                        Learn More
                    </a>
                </p>
            </div>
        </div>
        <div class="item">
            <div class="carousel-item3"></div>
            <div class="carousel-caption">
                <p>
                    See with organisations are linked your account.
                    <a class="btn btn-default btn-default" href="#">
                        Learn More
                    </a>
                </p>
            </div>
        </div>
        <div class="item">
            <div class="carousel-item4"></div>
            <div class="carousel-caption">
                <p>
                    Learn the importance of your identity information.
                    <a class="btn btn-default btn-default" href="#">
                        Learn More
                    </a>
                </p>
            </div>
        </div>
    </div>
    <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>


<div class="row">
    <div class="col-md-12 company-slogan">
        <div class="container">
            <h1>Moving Houses?</h1>
            <h2>
                Update your address here and let us do the rest for <b>you</b>! 
                @*The solution to your hassle of updating your address and contact info to organisations you are subscribed to.*@
            </h2>
        </div>
    </div>
</div>
