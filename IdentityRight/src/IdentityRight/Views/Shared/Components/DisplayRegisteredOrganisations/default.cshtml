@model IEnumerable<IdentityRight.Models.ApplicationOrganisations>
<br />
<h2>Connected Organisations</h2>
@if (Model.Count() != 0)
{
    <ul class="list-group">
        @foreach (var item in Model)
        {
            <li class="list-group-item">@Html.DisplayFor(modelItem => item.organisationName)</li>
        }
    </ul>
}
else
{
    <text> You have no current connections </text>

}
